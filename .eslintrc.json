{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "arrow-parens": ["error", "as-needed", { "requireForBlockBody": true }],
        "max-len": ["error", 160],
        "no-underscore-dangle": [
            "error",
            { "allow": ["_etag", "_cache", "__schemaVersion", "_count"] }
        ],
        "object-curly-newline": ["off"]
    }
}
